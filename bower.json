{
  "name": "HelloIonic",
  "private": "true",
  "devDependencies": {
    "ionic": "driftyco/ionic-bower#1.3.1"
  },
  "dependencies": {
    "jquery": "3.2.1",
    "inputmask": "3.3.7",
    "jquery.maskedinput": "1.4.1",
    "blockUI": "v2.65",
    "font-awesome": "4.7.0",
    "moment": "2.18.1"
  },
  "exportsOverride": {
    "jquery": {
      "js": "dist/*"
    },
    "inputmask": {
      "js": [
        "dist/*.js",
        "dist/min/*.js",
        "dist/inputmask/*.js",
        "dist/min/inputmask/*.js"
      ]
    },
    "blockUI": {
      "js": "*.js"
    },
    "font-awesome": {
      "css": "css/*",
      "fonts": "fonts/*"
    },
    "moment": {
      "js": [
        "moment.js",
        "min/moment.min.js"
      ]
    }
  },
  "resolutions": {
    "blockUI": "v2.65"
  }
}
